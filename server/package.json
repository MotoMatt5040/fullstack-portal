{
  "name": "fullstack-portal-server",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo 'Server build completed'",
    "test": "jest",
    "test:run": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "clean": "rm -rf node_modules logs",
    "logs": "tail -f logs/app.log",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:auto": "sequelize-auto -h localhost -d database_name -u username -x password -p 1433 --dialect mssql -o ./models",
    "generate:secret": "node -e \"console.log(require('crypto').randomBytes(64).toString('hex'))\""
  },
  "keywords": [
    "express",
    "nodejs",
    "api",
    "mongodb",
    "mssql",
    "sequelize"
  ],
  "author": "MotoMatt5040",
  "license": "ISC",
  "description": "Fullstack Portal Server API with MongoDB and MSSQL support",
  "dependencies": {
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.10.0",
    "mssql": "^11.0.1",
    "multer": "^2.0.2",
    "nodemailer": "^6.10.0",
    "odbc": "^2.4.9",
    "openai": "^5.9.0",
    "sequelize": "^6.37.7",
    "tedious": "^18.6.1",
    "uuid": "^11.0.5",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "sequelize-auto": "^0.8.8",
    "supertest": "^7.1.3"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**",
      "!models/**"
    ]
  }
}
